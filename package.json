{
  "name": "ngx-trend",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "publish": "cd dist && npm publish",
    "bundlesize": "npx bundlesize",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint ngx-trend --type-check --format stylish",
    "lint:fix": "ng lint ngx-trend --type-check --fix --format stylish",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-trend/'",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist && semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/material": "6.4.7",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@ctrl/ngx-github-buttons": "2.0.4",
    "core-js": "2.5.7",
    "hammerjs": "2.0.8",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.2.5",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@angular-devkit/build-angular": "0.8.5",
    "@types/fs-extra": "5.0.4",
    "@types/jasmine": "2.8.9",
    "@types/node": "10.12.0",
    "codelyzer": "4.5.0",
    "fs-extra": "7.0.0",
    "jasmine-core": "3.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "ng-packagr": "4.3.1",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.17",
    "travis-deploy-once": "5.0.9",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "release": {
    "branch": "master"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ngx-trend.umd.js",
      "maxSize": "20 kB"
    },
    {
      "path": "./dist/bundles/ngx-trend.umd.min.js",
      "maxSize": "3 kB"
    },
    {
      "path": "./dist/esm5/ngx-trend.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/esm2015/ngx-trend.js",
      "maxSize": "5 kB"
    }
  ]
}
